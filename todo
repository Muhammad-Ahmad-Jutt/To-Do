sub window must be exit when main window in exited


CREATE TABLE "Task" (
	"Index"	INTEGER,
	"Name"	TEXT NOT NULL UNIQUE,
	"Day"	DATE,
	"Added"	DATE,
	"Status"	TEXT,
	"Reschedule"	TEXT,
	"occurance"	TEXT,
	PRIMARY KEY("Index" AUTOINCREMENT)
);



JULIANDAY(arrival) - JULIANDAY(departure) AS difference